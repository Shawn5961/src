#!/bin/bash
#
# Super Puzzle Box 2 Turbo edition
#

# Are you looking before you leap? Comment out or remove the following line.
#
#rm -f ${0}

if [ -e "datafile" ]; then
	if [ ! -x "datafile" ]; then
		if [ -r "datafile" ]; then
			echo    "Look at datafile in cat (pipe to less). Look for patterns."
			echo    "There will be 2 pieces of information you need to obtain  "
			echo    "in order to complete this puzzle box."
		else
			echo "Still some work to do."
			exit 2
		fi
	else
		echo "Not ready yet."
		exit 1
	fi
else
	echo "I see you've made it this far. The end is in sight."

	(umask 777; cat ${0} | grep '^#DATA=' | tail -1 > datafile)
	chmod +x datafile

	for value in `echo $USER | sed 's/\(.\)/\1 /g'`; do
		let sum=${sum}+`printf '%d' "'${value}"`
	done

	echo "When you figure it out, you want field: `bc <<< \"(${sum}%4)+2\"`"
	exit 4
fi

exit 0
